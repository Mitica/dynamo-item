{
  "name": "dynamo-item",
  "version": "0.1.0",
  "description": "DynamoDB item",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./lib/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitica/dynamo-item.git"
  },
  "keywords": [
    "dynamo",
    "dynamodb",
    "dunamo-model",
    "model",
    "item"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mitica/dynamo-item/issues"
  },
  "homepage": "https://github.com/Mitica/dynamo-item#readme",
  "devDependencies": {
    "@types/node": "10.12.3",
    "ava": "^0.25.0",
    "debug": "^4.1.0",
    "dynamodb-local": "^0.0.22",
    "rimraf": "^2.6.2",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "aws-sdk": "^2.351.0",
    "dynamo-input": "^0.1.0"
  }
}
